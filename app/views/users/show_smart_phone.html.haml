- if @user != current_user || @relation_user_type.nil?
  = render :partial => "user_table", :locals => { :user => @user }
- else
  = render :partial => "master_table", :locals => { :type => @type, :relation_user => @relation_user, :relation_user_type => @relation_user_type }

- if @user != current_user
  = render :partial => "deshiel", :locals => { :user => @user }
  = render :partial => "objectives", :locals => { :type => User::QUESTION_TYPE[:p_a], :entry => @entry, :objectives => @objectives }
- else
  = render :partial => "objectives", :locals => { :type => @type, :entry => @entry, :objectives => @objectives }